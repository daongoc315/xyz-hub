{
  "configuration": {
    "status": "info",

    "name": "Default Log Config",
    "packages": "com.here.xyz",

    "ThresholdFilter": {
      "level": "info"
    },
    "CustomLevels": {
      "CustomLevel": [{
        "name": "STREAM",
        "intLevel": 350
      }]
    },
    "appenders": {
      "Console": {
        "name": "STDOUT",
        "PatternLayout": {
          "pattern": "%d %-5p %c{1} %.-4096msg %enc{%ex}{JSON}%n%xEx{none}"
        },
        "DynamicThresholdFilter": {
          "key": "LogLvl",
          "defaultThreshold": "STREAM",
          "onMatch": "ACCEPT",
          "onMismatch": "DENY",
          "KeyValuePair": [
            {"key": "ALL","value": "ALL"},
            {"key": "TRACE","value": "TRACE"},
            {"key": "DEBUG","value": "DEBUG"},
            {"key": "INFO","value": "INFO"},
            {"key": "WARN","value": "WARN"},
            {"key": "ERROR","value": "ERROR"},
            {"key": "FATAL","value": "FATAL"},
            {"key": "OFF","value": "OFF"},
            {"key": "STREAM","value": "STREAM"}
          ]
        }
      }
    },
    "loggers": {
      "root": {
        "level": "info",
        "AppenderRef": {
          "ref": "STDOUT"
        }
      }
    }
  }
}
